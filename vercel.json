{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "file": "api/.env"
},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ]
  
    
  
}
